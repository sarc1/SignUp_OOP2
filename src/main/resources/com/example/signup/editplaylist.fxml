<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="pneditplaylist" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.signup.EditplaylistController">
   <children>
      <GridPane layoutX="159.0" layoutY="108.0" prefHeight="182.0" prefWidth="282.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnAddSong" mnemonicParsing="false" onAction="#onAddSongButtonClick" text="Add Song" />
            <Button fx:id="btnRemoveSong" mnemonicParsing="false" onAction="#onRemoveSongButtonClick" text="Remove Song" GridPane.rowIndex="1" />
            <Button fx:id="btnViewSongs" mnemonicParsing="false" onAction="#onViewSongButtonClick" text="View Songs" GridPane.rowIndex="2" />
            <Button fx:id="btnHomePage" mnemonicParsing="false" onAction="#onHomePageButtonClick" text="Home Page" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
