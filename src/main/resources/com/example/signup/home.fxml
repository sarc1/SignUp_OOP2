<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="pnHomePage" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.signup.HomeController">
   <children>
      <GridPane alignment="CENTER" layoutX="384.0" layoutY="162.0" prefHeight="179.0" prefWidth="121.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnChangeUsername" mnemonicParsing="false" onAction="#onChangeUsernameButtonClick" text="Change Username" />
            <Button fx:id="btnChangePassword" mnemonicParsing="false" onAction="#onChangePasswordButtonClick" text="Change Password" GridPane.rowIndex="1" />
            <Button fx:id="btnLogout" mnemonicParsing="false" onAction="#onLogoutButtonClick" text="Logout" GridPane.rowIndex="3" />
            <Button mnemonicParsing="false" text="Delete Account" GridPane.rowIndex="2" />
         </children>
      </GridPane>
      <GridPane layoutX="175.0" layoutY="108.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnEditPlaylist" mnemonicParsing="false" onAction="#onEditPlaylistButtonClick" text="Edit Playlist" />
            <Button mnemonicParsing="false" onAction="#onViewPlaylistButtonClick" text="View Playlist" GridPane.rowIndex="1" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
