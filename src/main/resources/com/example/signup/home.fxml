<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="pnHomePage" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.signup.HomeController">
   <children>
      <GridPane layoutX="55.0" layoutY="43.0" prefHeight="103.0" prefWidth="100.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="38.0" minHeight="10.0" prefHeight="36.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="40.0" minHeight="10.0" prefHeight="36.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="25.0" minHeight="10.0" prefHeight="18.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnEditPlaylist" mnemonicParsing="false" onAction="#onEditPlaylistButtonClick" style="-fx-background-color: #D397F8;" text="Edit Playlist">
               <font>
                  <Font name="Inter Regular" size="12.0" />
               </font>
               <GridPane.margin>
                  <Insets top="5.0" />
               </GridPane.margin></Button>
            <Button mnemonicParsing="false" onAction="#onViewPlaylistButtonClick" style="-fx-background-color: #D397F8;" text="View Playlist" GridPane.rowIndex="1">
               <font>
                  <Font name="Inter Regular" size="12.0" />
               </font>
               <GridPane.margin>
                  <Insets top="5.0" />
               </GridPane.margin></Button>
         </children>
      </GridPane>
      <Button fx:id="btnChangePassword" layoutX="350.0" layoutY="95.0" mnemonicParsing="false" onAction="#onChangePasswordButtonClick" style="-fx-background-color: #D397F8;" text="Change Password">
         <font>
            <Font name="Inter Regular" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnChangeUsername" layoutX="348.0" layoutY="51.0" mnemonicParsing="false" onAction="#onChangeUsernameButtonClick" style="-fx-background-color: #D397F8;" text="Change Username">
         <font>
            <Font name="Inter Regular" size="12.0" />
         </font>
      </Button>
      <Button layoutX="369.0" layoutY="272.0" mnemonicParsing="false" onAction="#onDeleteButtonClick" style="-fx-background-color: #D397F8;" text="Delete Account">
         <font>
            <Font name="Inter Regular" size="12.0" />
         </font>
      </Button>
      <Button fx:id="btnLogout" layoutX="418.0" layoutY="310.0" mnemonicParsing="false" onAction="#onLogoutButtonClick" style="-fx-background-color: #D397F8;" text="Logout">
         <font>
            <Font name="Inter Regular" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
