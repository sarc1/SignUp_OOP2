<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="pnChangeUsername" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.signup.UsernamechangeController">
   <children>
      <GridPane layoutX="146.0" layoutY="124.0" prefHeight="163.0" prefWidth="310.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="150.0" minWidth="10.0" prefWidth="123.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="187.0" minWidth="10.0" prefWidth="187.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Button fx:id="btnChange" mnemonicParsing="false" onAction="#onChangeButtonClick" text="Change" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Label text="Old Username">
               <font>
                  <Font name="Inter Regular" size="14.0" />
               </font></Label>
            <Label text="New Username" GridPane.rowIndex="2">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Text fx:id="oldusernameerror" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="188.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
               <font>
                  <Font name="Inter Regular" size="12.0" />
               </font>
            </Text>
            <Text fx:id="newusernameerror" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" wrappingWidth="188.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <TextField fx:id="oldusername" GridPane.columnIndex="1" />
            <TextField fx:id="newusername" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
